<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
	 	<setting name="mapUnderscoreToCamelCase" value="true"/>
	 	<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
 
	<typeAliases>
		<!-- <typeAlias type="kh.spring.s01.board.model.vo.BoardVo" alias="BoardVo"/> -->
		<typeAlias type="com.kh.everyrecipe.member.vo.MemberVo" alias="mvo"/>
		<typeAlias type="com.kh.everyrecipe.comment.vo.CommentVo" alias="cvo"/>
		<typeAlias type="com.kh.everyrecipe.board.vo.BoardVo" alias="bvo"/>
		<typeAlias type="com.kh.everyrecipe.board.vo.IngredientVo" alias="ivo"/>
		<typeAlias type="com.kh.everyrecipe.board.vo.HashtagVo" alias="hvo"/>
