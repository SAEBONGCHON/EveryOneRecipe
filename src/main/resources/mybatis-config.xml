		<mapper resource="mappers/postBookmark-mapper.xml"/>
		<mapper resource="mappers/weekboard-mapper.xml"/>
		<mapper resource="mappers/replycomment-mapper.xml"/>
		<mapper resource="mappers/boardsearch-mapper.xml"/>
		<mapper resource="mappers/report-mapper.xml"/>
		<mapper resource="mappers/chat-mapper.xml"/>
	</mappers>
	
</configuration>
